<%= form_with(model: interest, local: true) do |f| %>
  
  <%= render 'layouts/error_messages', model: f.object %>

  <%= f.text_field :name, placeholder: "ギフトアイデアの名称" %>
  <%= f.file_field :image %>
  <div id="image-list"></div>
  <%= f.text_area :memo, placeholder: "メモ", rows: "10" %>
  <%= f.submit "登録" %>
<% end %>
